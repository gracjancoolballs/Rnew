local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "KeySystem", HidePremium = false, SaveConfig = true, IntroEnabled = false})

OrionLib:MakeNotification({
	Name = "Logged In!",
	Content = "You are logged in as "..Player.Name.." ",
	Image = "http://www.roblox.com/asset/?id=11089513932",
	Time = 5
})

_G.Key = "SUMMERIN2WEEKSAND3DAYS!"
_G.KeyInput = "string"
_G.PremiumKey = "TOMEK2009"
_G.PremiumKeyInput = "string"

function CorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "You have entered the correct key!",
        Image = "http://www.roblox.com/asset/?id=11637565033",
        Time = 5
    })
end

function CorrectPremiumKeyNotification()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "Welcome Premium Member!",
        Image = "http://www.roblox.com/asset/?id=9281670719",
        Time = 5
    })
end

function IncorrectKeyNotification()
    OrionLib:MakeNotification({
        Name = "Incorrect Key!",
        Content = "You have entered Wrong key!",
        Image = "http://www.roblox.com/asset/?id=11637565033",
        Time = 5
    })
end





function MakePremiumScriptHub()
    local Window = OrionLib:MakeWindow({Name = "TomScripts-PREMIUM", HidePremium = false, IntroText = "TomScripts PREMIUM HEHEH", SaveConfig = true, ConfigFolder = "OrionTest"})
    local MainTab = Window:MakeTab({
        Name = "Main",
        Icon = "http://www.roblox.com/asset/?id=9281670719",
        PremiumOnly = false
    })






    local SL2Tab = Window:MakeTab({
        Name = "SL2",
        Icon = "http://www.roblox.com/asset/?id=7625033269",
        PremiumOnly = false
    })
    
    SL2Tab:AddButton({
        Name = "EXECUTE SCRIPT!",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/MonkeyHUBB/MonkeyHUB/main/README.md"))()
        end  
    })

    MainTab:AddButton({
        Name = "Admin!",
        Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/raw/KtyqgNCD"))()
          end    
    })


    end






function MakeScriptHub()
    local Window = OrionLib:MakeWindow({Name = "TomScripts", HidePremium = false, IntroText = "TomScripts HAHAHA", SaveConfig = true, ConfigFolder = "OrionTest"})
    local MainTab = Window:MakeTab({
        Name = "Main",
        Icon = "http://www.roblox.com/asset/?id=5076713302",
        PremiumOnly = false
    })






    local SL2Tab = Window:MakeTab({
        Name = "SL2",
        Icon = "http://www.roblox.com/asset/?id=7625033269",
        PremiumOnly = false
    })
    
    SL2Tab:AddButton({
        Name = "EXECUTE SCRIPT!",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/MonkeyHUBB/MonkeyHUB/main/README.md"))()
        end  
    })

    MainTab:AddButton({
        Name = "Admin!",
        Callback = function()
            loadstring(game:HttpGet("https://pastebin.com/raw/KtyqgNCD"))()
          end    
    })


    end


local KeyTab = Window:MakeTab({
	Name = "Login",
	Icon = "http://www.roblox.com/asset/?id=12342116474",
	PremiumOnly = false
})

local PremiumloginTab = Window:MakeTab({
	Name = "PremiumLogin",
	Icon = "http://www.roblox.com/asset/?id=9281670719",
	PremiumOnly = false
})


PremiumloginTab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.PremiumKeyInput = Value
	end	  
})



PremiumloginTab:AddButton({
	Name = "CheckKey",
	Callback = function()
      	if _G.PremiumKeyInput == _G.PremiumKey then
            CorrectPremiumKeyNotification()
            MakePremiumScriptHub()
        else
            IncorrectKeyNotification()
        end
    end  
}) 





KeyTab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

KeyTab:AddButton({
	Name = "CheckKey",
	Callback = function()
      	if _G.KeyInput == _G.Key then
            CorrectKeyNotification()
            MakeScriptHub()
        else
            IncorrectKeyNotification()
        end
    end  
})
OrionLib:Init()
